{
	"name": "@ctx-core/checkbox-tree-ui-svelte",
	"version": "0.2.81",
	"description": "ctx-core checkbox tree component support library",
	"keywords": [
		"ctx-core",
		"Checkbox_Tree",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/checkbox-tree-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/checkbox-tree-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/checkbox-tree-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Checkbox_Tree",
		"./Checkbox_Tree_Input",
		"./Checkbox_Tree_Knob",
		"./Checkbox_Tree_Node"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/svelte": "^16.0.27",
		"svelte": "^4.2.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"c8": "^9.0.0",
		"check-dts": "^0.7.2",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}